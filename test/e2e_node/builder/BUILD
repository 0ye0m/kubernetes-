package(default_visibility = ["//visibility:public"])

load(
    "//build:go.bzl",
    "go_library",
)

go_library(
    name = "go_default_library",
    srcs = ["build.go"],
    importpath = "k8s.io/kubernetes/test/e2e_node/builder",
    deps = ["//vendor/github.com/golang/glog:go_default_library"],
)

