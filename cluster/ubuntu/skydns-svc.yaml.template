kind: Service
apiVersion: v1beta1
id: kubernetes-dns
namespace: default
protocol: UDP
port: 53
portalIP: {{ pillar['dns_server'] }}
containerPort: 53
labels:
  k8s-app: kube-dns
  name: kube-dns
  kubernetes.io/cluster-service: "true"
annotations:
  kubernetes.io/cluster-domain: {{ pillar['dns_domain'] }}
selector:
  k8s-app: kube-dns
